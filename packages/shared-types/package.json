{
  "name": "@greenenergy/shared-types",
  "version": "0.1.0",
  "private": true,
  "description": "Shared TypeScript types and interfaces for the Green Energy Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
